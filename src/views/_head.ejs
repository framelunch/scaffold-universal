<!doctype html>
<html lang="ja">
<head>
    <meta charset="UTF-8">

    <title><%=conf.title%></title>
    <meta name="description" content="<%=conf.description%>">

    <meta property="og:title" content="" />
    <meta property="og:description" content="" />
    <meta property="og:type" content="website" />
    <meta property="og:url" content="" />
    <meta property="og:site_name" content="" />
    <meta property="og:image" content="" />

    <meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1,user-scalable=no">

    <%
    conf.script = conf.script || [];
    if (typeof conf.script === 'string') { conf.script = [conf.script]; }

    conf.style = conf.style || [];
    if (typeof conf.style === 'string') { conf.style = [conf.style]; }
    %>

    <% for (let i=0; i < conf.style.length; i++) { %>
    <link rel="stylesheet" href="/css/<%= conf.style[i] %>">
    <% } %>

</head>
<body>
